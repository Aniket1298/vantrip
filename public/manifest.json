{
    "$schema": "https://json.schemastore.org/web-manifest-combined.json",
    "name": "TimelessKashi - Varanasi Tour Packages",
    "short_name": "TimelessKashi",
    "description": "Curated Varanasi tour packages, Ganga Aarti experiences, temple tours, and boat rides.",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#fffbf2",
    "theme_color": "#d97706",
    "icons": [
        {
            "src": "/favicon.ico",
            "sizes": "32x32",
            "type": "image/x-icon"
        },
        {
            "src": "/logo.jpeg",
            "sizes": "192x192",
            "type": "image/jpeg"
        },
        {
            "src": "/logo.jpeg",
            "sizes": "512x512",
            "type": "image/jpeg"
        },
        {
            "src": "/logo.jpeg",
            "sizes": "any",
            "type": "image/jpeg",
            "purpose": "any maskable"
        }
    ]
}